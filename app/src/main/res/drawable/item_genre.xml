<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:left="@dimen/_16mdp">
        <selector>
            <item android:state_selected="false">
                <shape>
                    <stroke android:width="1dp" android:color="@color/purple2_text_color" />
                    <corners android:radius="12dp" />
                </shape>
            </item>
            <item android:state_selected="true">
                <shape>
                    <solid android:color="@color/purple2_text_color" />
                    <corners android:radius="12dp" />
                </shape>
            </item>
        </selector>
    </item>


</layer-list>
